.oneclicktheme-header{background:#fff}.oneclicktheme-header .download-btn{background:linear-gradient(65deg,#FF5E5B 0,#F8778D 100%);-webkit-transition:background .15s ease;transition:background .15s ease;color:#fff}.oneclicktheme-header .download-btn:after{content:'';border-radius:2px;position:absolute;top:0;right:0;bottom:0;left:0;-webkit-box-shadow:0 8px 16px rgba(255,94,91,.25);box-shadow:0 8px 16px rgba(255,94,91,.25);mix-blend-mode:multiply;-webkit-transition:-webkit-box-shadow .15s ease;transition:-webkit-box-shadow .15s ease;transition:box-shadow .15s ease;transition:box-shadow .15s ease,-webkit-box-shadow .15s ease}.oneclicktheme-header .download-btn:hover:after{-webkit-box-shadow:0 8px 16px rgba(255,94,91,.4);box-shadow:0 8px 16px rgba(255,94,91,.4)}.section-banner .title{font-size:5rem}.section-banner .profile-img{width:250px;height:360px;-o-object-fit:cover;object-fit:cover;-o-object-position:0 0;object-position:0 0}.custom-border:before{content:'';border:1px solid var(--color-text-primary);display:inline-block;width:100%;height:100%;position:absolute;top:-20px;bottom:0;left:-20px;right:0;border-radius:8px;z-index:-1;opacity:.4}